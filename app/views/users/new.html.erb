<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for:user,url: users_path do |f| %>

<p>
<%= f.label:username %><br>
<%= f.text_field(:username, {:class=>'form-control'}) %>
</p>

<p>
<%= f.label:password %><br>
<%= f.text_field(:password, {:class=>'form-control'}) %>
</p>


<p>
<%= f.submit({:class=> 'btn btn-primary'}) %>
</p>

<%end%>
